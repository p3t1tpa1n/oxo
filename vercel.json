{
  "name": "oxo",
  "version": 2,
  "public": false,
  "framework": null,
  "buildCommand": "flutter build web --release",
  "outputDirectory": "build/web",
  "installCommand": "flutter pub get",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "pubspec.yaml",
      "use": "vercel-flutter"
    }
  ]
} 